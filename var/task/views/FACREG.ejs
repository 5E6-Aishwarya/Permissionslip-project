    <style>
        .bg{
                margin-top: 5%;
                margin-left: 35%;
                padding-top:1%;
                padding-left:1%;
                margin-right:35%;
                border-radius:5px;
                border-style:solid;
            }
            th,td {
                width: 1rem;
                text-align: center;
                padding: 0.5%;
                padding-bottom: 4%;
            }
            .class1{
                margin-left: 50%;
            }

    </style>
    <body>
        <h1>Faculty Registration</h1>
        <form action="/facultySubmit" method="get">
        <table>
            <tr>
                <th>First Name</th>
                <td><input type="text" name="fname" required></td>
            </tr>
            <tr>
                <th> Last Name</th>
                <td><input type="text" name="lname" required></td>
            </tr>
            <tr>
                <th>Email</th>
                <td><input type="email" name="email" required></td>
            </tr>
            <tr>
                <th>Department_id</th>
                <td><select name="sel">
                    <option>02</option>
                    <option>04</option>
                    <option>05</option>
                    <option>12</option>
                    <option>66</option>
                </select></td><br>
            </tr>
            <tr>
                <th>Year</th>
                <td><input type="number" name="year" required></td>
            </tr>
            <tr>
                <th>Section</th>
                <td><input type="text" name="section" required></td>
            </tr>
            <tr>
                <th>Password</th>
                <td><input type="password" name="pwd" required></td>
            </tr>
            <tr>
                <td><button class="class1 btn btn-light">Register</button></td>
            </tr>
        </table>
    </form>
    </body>
</html>
